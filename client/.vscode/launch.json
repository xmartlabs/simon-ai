  "version": "0.2.0",
  "configurations": [
    {
      "name": "ClientApp Staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor=staging",
        "--dart-define=ENV=staging"
      ]
    },
    {
      "name": "ClientApp Production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor=production",
        "--dart-define=ENV=production"
      ]
    },
    {
      "name": "ClientApp (profile mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--profile",
        "--flavor=staging",
        "--dart-define=ENV=staging",
      ],
    },
    {
      "name": "ClientApp (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Integration Test",
      "program": "integration_test/",
      "request": "launch",
      "type": "dart"
    }
  ]
}